{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/eligibility",
      "dest": "/app/api/eligibility/route.js"
    },
    {
      "src": "/api/passport",
      "dest": "/app/api/passport/route.js"
    },
    {
      "src": "/api/submit-passport",
      "dest": "/app/api/submit-passport/route.js"
    }
  ]
}
