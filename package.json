{
  "name": "se-2",
  "version": "0.0.1",
  "private": true,
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "scripts": {
    "account": "pnpm --filter @se-2/foundry account",
    "account:generate": "pnpm --filter @se-2/foundry account:generate",
    "account:import": "pnpm --filter @se-2/foundry account:import",
    "chain": "pnpm --filter @se-2/foundry chain",
    "compile": "pnpm --filter @se-2/foundry compile",
    "deploy": "pnpm --filter @se-2/foundry deploy",
    "deploy:verify": "pnpm --filter @se-2/foundry deploy:verify",
    "flatten": "pnpm --filter @se-2/foundry flatten",
    "fork": "pnpm --filter @se-2/foundry fork",
    "format": "yarn next:format && yarn foundry:format",
    "foundry:format": "pnpm --filter @se-2/foundry format",
    "foundry:lint": "pnpm --filter @se-2/foundry lint",
    "foundry:test": "pnpm --filter @se-2/foundry test",
    "generate": "yarn account:generate",
    "graph": "pnpm --filter @se-2/subgraph graph",
    "graphclient:build": "pnpm --filter @se-2/nextjs client",
    "postinstall": "husky install",
    "next:build": "pnpm --filter @se-2/nextjs build",
    "next:check-types": "pnpm --filter @se-2/nextjs check-types",
    "next:format": "pnpm --filter @se-2/nextjs format",
    "next:lint": "pnpm --filter @se-2/nextjs lint",
    "next:serve": "pnpm --filter @se-2/nextjs serve",
    "precommit": "lint-staged",
    "start": "pnpm --filter @se-2/nextjs dev",
    "subgraph:abi-copy": "pnpm --filter @se-2/subgraph abi-copy",
    "subgraph:build": "pnpm --filter @se-2/subgraph build",
    "subgraph:clean-node": "pnpm --filter @se-2/subgraph clean-node",
    "subgraph:codegen": "pnpm --filter @se-2/subgraph codegen",
    "subgraph:create-local": "pnpm --filter @se-2/subgraph create-local",
    "subgraph:local-ship": "pnpm --filter @se-2/subgraph local-ship",
    "subgraph:run-node": "pnpm --filter @se-2/subgraph run-node",
    "subgraph:stop-node": "pnpm --filter @se-2/subgraph stop-node",
    "subgraph:test": "pnpm --filter @se-2/subgraph test",
    "test": "yarn foundry:test",
    "vercel": "pnpm --filter @se-2/nextjs vercel",
    "vercel:yolo": "pnpm --filter @se-2/nextjs vercel:yolo",
    "verify": "pnpm --filter @se-2/foundry verify"
  },
  "devDependencies": {
    "husky": "~8.0.3",
    "lint-staged": "~13.2.2"
  },
  "packageManager": "yarn@3.2.3",
  "engines": {
    "node": ">=18.18.0"
  }
}
